{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "global-require": "off",
      "react/jsx-props-no-spreading": "off",
      "import/no-named-as-default": "off",
      "import/prefer-default-export": "off",
      "react/jsx-fragments": "off"
    }
  }
